{
  "endpoint": "/api/v2/generate-quote-reply",
  "tests": [
    {
      "name": "A) Pricing (must hit)",
      "question": "How much is a basic detail?",
      "expect_debug_branch_any": ["fact_hit", "fact_rewrite_hit"]
    },
    {
      "name": "B) Service area (must hit)",
      "question": "Do you cover Brisbane north?",
      "expect_debug_branch_any": ["fact_hit", "fact_rewrite_hit"]
    },
    {
      "name": "C) Water/power (must hit)",
      "question": "Do you need water and power?",
      "expect_debug_branch_any": ["fact_hit", "fact_rewrite_hit"]
    },
    {
      "name": "D) Mobile (must hit)",
      "question": "Are you mobile / do you come to me?",
      "expect_debug_branch_any": ["fact_hit", "fact_rewrite_hit"]
    },
    {
      "name": "E) Payments (must hit)",
      "question": "Do you take card or bank transfer?",
      "expect_debug_branch_any": ["fact_hit", "fact_rewrite_hit"]
    },
    {
      "name": "F) Unknown capability (miss)",
      "question": "can you fix my transmission?",
      "expect_debug_branch_any": ["fact_miss"]
    },
    {
      "name": "G) Unknown capability (miss)",
      "question": "can you do ceramic coating?",
      "expect_debug_branch_any": ["fact_miss"]
    },
    {
